<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>True</UseWindowsForms>
    <StartupObject>EditorImagenes.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="acercarse.png" />
    <None Remove="ayuda.png" />
    <None Remove="buscar.png" />
    <None Remove="busqueda.png" />
    <None Remove="Captura.png" />
    <None Remove="cerrar-sesion.png" />
    <None Remove="flechaCurvaDerecha.png" />
    <None Remove="flechaCurvaIzquierda.png" />
    <None Remove="gira-a-la-derecha.png" />
    <None Remove="girar-a-la-izquierda.png" />
    <None Remove="Interrogacion.png" />
    <None Remove="Placeholder_view_vector.svg.png" />
    <None Remove="Salir.png" />
    <None Remove="titulo.png" />
    <None Remove="zoomas.png" />
    <None Remove="zoomenos.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="flechaCurvaDerecha.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="flechaCurvaIzquierda.png" />
    <Resource Include="Interrogacion.png" />
    <Resource Include="Placeholder_view_vector.svg.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="acercarse.png" />
    <Resource Include="ayuda.png" />
    <Resource Include="buscar.png" />
    <Resource Include="busqueda.png" />
    <Resource Include="Captura.png" />
    <Resource Include="cerrar-sesion.png" />
    <Resource Include="gira-a-la-derecha.png" />
    <Resource Include="girar-a-la-izquierda.png" />
    <Resource Include="Salir.png" />
    <Resource Include="titulo.png" />
    <Resource Include="zoomas.png" />
    <Resource Include="zoomenos.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
